{
  "name": "Jade Support",
  "version": "0.9.10.3",
  "manifest_version": 2,
  "minimum_chrome_version": "44",
  "description": "The Jade Robotâ„¢, from Mimetics, is an advanced yet easy-to-use educational robot designed for youth, educators and school groups.",

  "icons": {
    "128": "img/128x128.png"
  },

  "app": {
    "background": {
      "scripts": ["ide-entry.js"]
    }
  },
  "key": "",

  "permissions": [
    "contextMenus",
    "webview",
    "identity",
    "serial",
    "storage",
    "fileSystem",
    "clipboardRead",
    "clipboardWrite",
    "https://accounts.google.com/*",
    "https://userdata.cloud.mimetics.ca/*",
    "https://toksvc.cloud.mimetics.ca/*",
    "https://avatars.githubusercontent.com/*"
   ],

  "bluetooth":{
    "socket": true,
    "uuids":
    [
        "00001101-0000-1000-8000-00805f9b34fb"
    ]
  },

  "sandbox": {
    "pages": ["blocklyframe.html"]
  },

  "oauth2": {
    "client_id": "453803291486.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.profile", "https://www.googleapis.com/auth/userinfo.email"]
  }

}